make-cont = (f): (x, y, cont): cont(f(x, y))

+' = make-cont(+)
-' = make-cont(-)
*' = make-cont(*)
/' = make-cont(/)

+'(10, 2,
    (x): -'(x, 3,
        (x): *'(x, 4,
            (x): /'(x, 2, <<<))))

fib-cont-impl = (self, n, cont):
    ?(le(n, 2),
        (): cont(1),
        (): self(self, -(n, 1),
            (x): self(self, -(n, 2),
                (y): cont(+(x, y)))))
fib-cont = (n, cont): fib-cont-impl(fib-cont-impl, n, cont)

fib-cont(>>>(), <<<)
